{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./",
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs|lib)"
  },
  "plugins": [
    "plugins/markdown", "./node_modules/jsdoc-strip-async-await"
  ],
  "opts": {
    "template": "./node_modules/docdash/",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}